{
  "manifest_version": 2,
  "version": "0.0.7",
  "name": "Wakarank",
  "description": "Wakatime user rank",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html",
    "default_title":"Wakarank"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": ["js/jquery.min.js", "js/content.js"],
      "matches": ["*://*/*"],
      "run_at": "document_start"
    }
  ]
}